{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "generateOptions": {
    "spec": false
  },
  "paths": {
    "modulesDir": "src/api/controllers",
    "servicesDir": "src/application/services",
    "controllersDir": "src/api/controllers"
  },
  "compilerOptions": {
    "deleteOutDir": true
  }
}
